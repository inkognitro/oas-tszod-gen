{
  "name": "oas2ts",
  "version": "0.0.1",
  "description": "A tool to convert OpenApi specifications into callable endpoint functions in TS.",
  "main": "index.js",
  "scripts": {
    "start": "npx tspc -w",
    "prodTest": "node build/src/generate.js",
    "test": "jest",
    "tspc": "tspc",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "npm.cmd run compile",
    "pretest": "npm.cmd run compile",
    "posttest": "npm.cmd run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inkognitro/oas2ts.git"
  },
  "keywords": [
    "oas2ts",
    "oas3",
    "ts-api"
  ],
  "author": "Marcel Fischer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/inkognitro/oas2ts/issues"
  },
  "homepage": "https://github.com/inkognitro/oas2ts#readme",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "20.8.2",
    "@types/uuid": "^9.0.8",
    "gts": "^5.2.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-patch": "^3.1.2",
    "typescript": "~5.2.0",
    "typescript-transform-paths": "^3.4.7"
  }
}
