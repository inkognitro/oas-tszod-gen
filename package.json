{
  "name": "@inkognitro/oas-to-code",
  "version": "0.0.8",
  "files": [
    "./badges",
    "./dist",
    "./LICENSE",
    "./package.json",
    "./README.md"
  ],
  "paths": {
    "@oas3/*": [
      "./dist/oas3/*"
    ]
  },
  "main": "dist/index.js",
  "description": "A tool to convert OpenApi specifications into callable endpoint functions in TS.",
  "scripts": {
    "lint": "gts lint",
    "clean": "gts clean",
    "copyTemplateFiles": "copyfiles -u 1 src/templates/** dist",
    "compile": "tsc && npm run copyTemplateFiles",
    "fix": "tsc && gts fix",
    "posttest": "npm run lint",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "prodTest": "node dist/generate.js",
    "start": "npx tspc -w && (concurrently \"tsc -w\" \"tsc-alias -w\")",
    "test": "jest",
    "test-ci": "jest --ci --coverage",
    "tspc": "tspc && tsc-alias && npm run copyTemplateFiles"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inkognitro/oas-to-code.git"
  },
  "keywords": [
    "oas-to-code",
    "oas3",
    "oas3 code generator"
  ],
  "author": "Marcel Fischer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/inkognitro/oas-to-code/issues"
  },
  "homepage": "https://github.com/inkognitro/oas-to-code#readme",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "20.8.2",
    "@types/uuid": "^9.0.8",
    "copyfiles": "^2.4.1",
    "gts": "^5.2.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-patch": "^3.1.2",
    "tsc-alias": "^1.8.8",
    "typescript": "~5.2.0",
    "typescript-transform-paths": "^3.4.7"
  },
  "dependencies": {
    "mkdirp": "^3.0.1"
  }
}
