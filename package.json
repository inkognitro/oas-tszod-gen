{
  "name": "@inkognitro/oas-to-code",
  "version": "0.0.21",
  "engines": {
    "node": ">=10.0.0"
  },
  "files": [
    "./dist",
    "./LICENSE",
    "./package.json",
    "./README.md"
  ],
  "paths": {
    "@/*": [
      "./dist/*"
    ]
  },
  "main": "dist/index.js",
  "description": "A tool to convert OpenApi specifications into callable endpoint functions in TS.",
  "scripts": {
    "build": "tspc && npm run copyTemplateFiles",
    "copyTemplateFiles": "copyfiles -u 1 src/templates/** dist",
    "lint": "gts lint",
    "lint:fix": "tsc --noEmit && gts fix",
    "start": "npm run copyTemplateFiles && tspc -w",
    "test": "jest",
    "test:withCoverage": "jest --ci --coverage",
    "tsc": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inkognitro/oas-to-code.git"
  },
  "keywords": [
    "oas-to-code",
    "oas3",
    "oas3 code generator",
    "oas to zod"
  ],
  "author": "Marcel Fischer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/inkognitro/oas-to-code/issues"
  },
  "homepage": "https://github.com/inkognitro/oas-to-code#readme",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "20.8.2",
    "@types/qs": "^6.9.15",
    "copyfiles": "^2.4.1",
    "express": "^4.21.0",
    "gts": "^5.2.0",
    "jest": "^29.7.0",
    "multer": "^1.4.5-lts.1",
    "qs": "^6.13.0",
    "ts-jest": "^29.1.2",
    "ts-patch": "^3.1.2",
    "typescript": "~5.2.0",
    "typescript-transform-paths": "^3.4.7"
  },
  "dependencies": {
    "mkdirp": "^3.0.1"
  },
  "peerDependencies": {
    "axios": "^1.7.7",
    "zod": "^3.23.8"
  }
}
